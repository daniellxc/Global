
@model string

@if(Model!="")
{
<div class="alert alert-dismissable alert-info">
  <button type="button" class="close" data-dismiss="alert">×</button>
  <h3><strong>Atenção!</strong></h3>
    <br />
    <p>@Model</p>
    <br />
    <a class="btn btn-primary" onclick="javascript: history.back()">Voltar</a>
</div>
    
}