@model object
@{
    string _action = ViewBag.action;
    string _controller = ViewBag.controller;
 

}
<div id="painel_principal" class="panel panel-primary" >
    
  <div class="panel-heading">
    <h3 class="panel-title">Confirme a execução da ação...</h3>
      
  </div>
  <div class="panel-body">
    <input type="button" onclick  ="$('#painel_principal').hide()" class="btn btn-primary" value="Cancelar" id="botao_voltar" />&nbsp

     
      @* @Html.ActionLink("Confirmar", _action, _controller, Model, new { @class = "btn btn-primary" })*@
     
        @{string a = "";}
        @for (int i=0; i< ((string[])Model).Count(); i++){
            a += (((string[])Model)[0].Replace(" ", "").Replace("{", "").Replace("}", "").Replace(",","&"));
        }


      <a href='@Url.Content("~/"+_controller+"/"+_action)?@a' class="btn btn-primary">
          Confirmar
      </a>
      
          
          
         
  </div>
</div>

<script>
    document.getElementById("botao_voltar").focus();
</script>